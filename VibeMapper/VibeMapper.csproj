      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="MudBlazor" Version="8.14.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\VibeMapper.Application\VibeMapper.Application.csproj" />
    <ProjectReference Include="..\VibeMapper.Core\VibeMapper.Core.csproj" />
    <ProjectReference Include="..\VibeMapper.Infrastructure\VibeMapper.Infrastructure.csproj" />
  </ItemGroup>

</Project>
